  journal = {Elsevier},
  year = {2011},
  abstract = {In this paper we present a new variant of the localised function method
	used when modelling photonic crystal ?bres. This variant is based
	solely on HermiteGaussian expansion series, which introduces the
	possibility of directly modelling ?bres with ?nite claddings. We
	investigate the challenges and advantages of such a model and compare
	it to a proven modelling tool, namely the plane-wave method.},
  owner = {s181203},
