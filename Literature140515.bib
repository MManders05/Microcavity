@CONFERENCE{Picon19941034,
  author = {Picon, Laura L., Bright, Victor M., Kolesar, Edward S.},
  title = {Detecting low-intensity magnetic fields with a magnetostrictive fiber
	optic sensor},
  year = {1994},
  volume = {2},
  pages = {1034-1039},
  address = {Dayton, OH, USA},
  publisher = {IEEE, Piscataway, NJ, United States},
  abbrev_source_title = {IEEE Proc Natl Aerosp Electron Conf},
  abstract = {A magnetostrictive fiber optic Mach-Zehnder interferometric sensor
	that detects low-intensity magnetic fields was realized. The sensor
	utilizes 850nm single mode fiber optic cable and couplers. Optimization
	techniques were applied to the sensor to maximize its sensitivity,
	including: annealing the magnetostrictive material; enclosing the
	Helmholtz coil, sensing arm, and reference arm in a magnetic shielded
	chamber; and applying a DC bias to the Helmhotz coil's AC current
	component. The measurements indicated that the lowest magnetic flux
	density that could be detected was 0.3 mG. This was achieved using
	a Metglas ribbon-fiber configuration where ribbon's magnetization
	direction was aligned perpendicularly with respect to the applied
	52kHz AC magnetic field and a 5 volt DC bias. When this configuration
	was correspondingly operated with a 10Hz AC magnetic field and a
	5 volt DC bias, an 8.9 mG magnetic flux density was detected.},
  affiliation = {Air Force Inst of Technology, Wright-Patterson AFB, United States},
  coden = {NASEA},
  correspondence_address = {Picon, Laura L.; Air Force Inst of Technology, Wright-Patterson AFB,
	United States},
  document_type = {Conference Paper},
  journal = {IEEE Proceedings of the National Aerospace and Electronics Conference},
  keywords = {Magnetic fields; Magnetic variables measurement; Magnetization; Optimization;
	Sensitivity analysis, Low intensity magnetic fields; Magnetic flux
	density; Magnetostrictive fiber optic sensor; Superconducting quantum
	interference device, Fiber optic sensors},
  language = {English},
  owner = {s181203},
